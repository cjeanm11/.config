syntax on
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set ai
set number
set hlsearch
set ruler
highlight Comment ctermfg=green
let &t_SI.="\e[6 q" "SI = INSERT mode
let &t_SR.="\e[4 q" "SR = REPLACE mode
let &t_EI.="\e[2 q" "EI = NORMAL mode (ELSE)
set clipboard=unnamedplus









