{
  "features": {
    "inline_completion_provider": "none"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "cursor_blink": false,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "vim_mode": true,
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "copy_on_select": false,
    "option_as_meta": false,
    "button": false,
    "toolbar": {
      "title": false
    },
    "working_directory": "current_project_directory"
  }
}
